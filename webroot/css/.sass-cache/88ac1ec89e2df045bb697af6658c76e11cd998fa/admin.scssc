@linei:@template"ı!*{
	padding: 0px;
	margin: 0px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-font-smoothing: antialiased;
}

*:focus { outline: none; }

@import "mixins";

//////////
// Fonts
//////////

@font-face {
	font-family: 'gotham';
	src: url('fonts/gotham-light.eot');
	src: url('fonts/gotham-light.eot?#iefix') format('embedded-opentype'),
		 url('fonts/gotham-light.svg#gotham_light') format('svg'),
		 url('fonts/gotham-light.woff') format('woff'),
		 url('fonts/gotham-light.ttf') format('truetype');
	font-weight: lighter;
	font-style: lighter;
}

@font-face {
	font-family: 'gotham';
	src: url('fonts/gotham-book.eot');
	src: url('fonts/gotham-book.eot?#iefix') format('embedded-opentype'),
		 url('fonts/gotham-book.svg#gotham_book') format('svg'),
		 url('fonts/gotham-book.woff') format('woff'),
		 url('fonts/gotham-book.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'gotham';
	src: url('fonts/gotham-medium.eot');
	src: url('fonts/gotham-medium.eot?#iefix') format('embedded-opentype'),
		 url('fonts/gotham-medium.svg#gotham_medium') format('svg'),
		 url('fonts/gotham-medium.woff') format('woff'),
		 url('fonts/gotham-medium.ttf') format('truetype');
	font-weight: bold;
	font-style: bold;
}

/////////////////
// Global Stlyes
/////////////////

::-moz-selection { background: #9bb2c5; color: #fff; text-shadow: none; }
::selection { background: #9bb2c5; color: #fff; text-shadow: none; }

$offwhite: #eeeeee;
$darkgrey: #555555;
$darkblue: #282b2e;
$grey: #888888;
$blue: #4e5a71;
$red: #8c5555;
$green: #81b084;

h1{
	color: white;
	font: lighter 20px 'gotham';
	text-align: left;
}

h2{
	color: white;
	font: bold 18px 'gotham';
	text-align: left;
}

h3{
	color: $darkgrey;
	font: normal 14px 'gotham';
	text-align: left;
}

p{
	color: $grey;
	font: normal 12px 'gotham';
	line-height: 18px;
	text-align: left;
}

a{
	color: $blue;
	text-decoration: none;
	&:hover{ text-decoration: underline;}
}

.button-blue{
	background: $blue;
	width: 90px;
	padding: 8px 0 6px;
	color: white;
	cursor: pointer;
	font: normal 14px 'gotham';
	float: right;
	text-align: center;
	margin-right: 10px;
	@include transition(0.3s all);
	&:hover{ background: #657799; }
}

.button-red{
	background: $red;
	width: 90px;
	padding: 8px 0 6px;
	color: white;
	cursor: pointer;
	font: normal 14px 'gotham';
	float: right;
	text-align: center;
	@include transition(0.3s all);
	&:hover{ background: #b05858; }
}

.button-green{
	background: $green;
	width: 90px;
	padding: 8px 0 6px;
	color: white;
	cursor: pointer;
	font: normal 14px 'gotham';
	float: right;
	text-align: center;
	@include transition(0.3s all);
	&:hover{ background: #98c59a; }
}

table{
	thead{
		background: url(../img/header-bg.png);
		height: 39px;
		border-radius: 5px;
		th{
			color: white;
			padding: 13px 14px 9px;
			font: normal 14px 'gotham';
			text-align: left;
			border-left: 1px solid #181a1b;
			border-right: 1px solid #4b4d52;
		}
		th:first-child{
			border-left: none;
		}
		th:last-child{
			border-right: none;
		}
	}
	tr{
		td{
			padding: 14px 20px 12px;
			color: $grey;
			font: normal 14px 'gotham';	
		}
	}
	tr:nth-child(2n+2){
		background: #f5f5f5;
		border-bottom: 1px solid #e4e4e4;
	}
}


/////////////
// Content
/////////////

html,body{
	width: 100%;
	height: 100%;
}

body{
	background: #585f69; /* Old browsers */
	background: -moz-radial-gradient(center, ellipse cover,  #585f69 0%, #292e33 100%); 
	background: -webkit-radial-gradient(center, ellipse cover,  #585f69 0%,#292e33 100%);
	background: -o-radial-gradient(center, ellipse cover,  #585f69 0%,#292e33 100%);
	background: radial-gradient(ellipse at center,  #585f69 0%,#292e33 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#585f69', endColorstr='#292e33',GradientType=1 );
}

#sidebar{
	background: $darkblue url(../img/header-shine.png) no-repeat;
	width: 230px;
	height: 100%;
	float: left;
	box-shadow: 2px 0px 4px 0px rgba(0, 0, 0, 0.2);
	-webkit-box-shadow: 2px 0px 4px 0px rgba(0, 0, 0, 0.2);
	#user{
		margin: 38px 0 0 22px;
		#profile-image{
			float: left;
			padding: 0 15px 0 0;
		}
		h2{
			margin-bottom: 5px;
		}
		p,a{
			color: white;
			cursor: pointer;
			&:hover{ text-decoration: underline;}
		}
	}
}

nav{
	margin-top: 50px;
	.nav-item{
		display: block;
		height: 34px;
		margin: 8px 0;
		padding: 10px 22px;
		font: 14px 'gotham';
		color: white;
		cursor: pointer;
		&:hover{
			background: url(../img/nav-item-bg.png);
			text-decoration: none;
			-webkit-box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, 0.2);
			box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, 0.2);
		}
	}
	.active{
		background: url(../img/nav-item-bg.png);
		text-decoration: none;
		-webkit-box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, 0.2);
		box-shadow:  0px 1px 3px 0px rgba(0, 0, 0, 0.2);
	}
}


#content{
	position: absolute;
	background: white;
	top: 30px;
	left: 280px;
	box-shadow: 1px 1px 3px 0px rgba(0, 0, 0, 0.2);
	-webkit-box-shadow: 1px 1px 3px 0px rgba(0, 0, 0, 0.2);
}

header{
	width: 100%;
	height: 39px;
	color: white;
	font: 14px 'gotham';
	padding: 13px 0;
	margin: 0 0 20px 0;
}


#newPost{
	background: url(../img/header-bg.png) repeat-x;
	display:none;
	overflow: auto;
	
	#newPost-left{
		overflow: auto;
		padding: 0 0 20px 26px;

		#postHeading, #postBody{
			display: block;
			background: #f4f4f4;
			width: 535px;
			height: 50px;
			padding: 15px;
			border: none;
			-webkit-box-shadow: inset 1px 1px 3px 0px rgba(0, 0, 0, 0.1);
			box-shadow: inset 1px 1px 3px 0px rgba(0, 0, 0, 0.1);
			color: $grey;
			font: bold 18px 'gotham';
		}
		#postBody{
			max-width: 535px;
			height: 273px;
			font: normal 14px 'gotham';
			float: left;
			margin-top: 15px;
		}
	}

	#newPost-right{
		width: 390px;
		float: right;
		padding: 0 26px 20px;

		header{
			border-left: 1px solid #181a1b, 1px solid #4b4d52;
		}

		#uploadImage, #savePost, #togglePreview{
			width: 134px;
			margin: 15px 0;
			float: none;
		}
		#uploadImage{
			position: relative;
			@include transition(0.3s all);
			input[type='file']{
				cursor: pointer;
				width: 134px;
				height: 31px;
				position: absolute;
				top: 0;
				left: 0;
				opacity: 0;
			}
		}
		.saved{
			background: $green;
		}
	}
}

#images{
	display: none;
}

#image-popup{
	display: none;
	position: fixed;
	background: url('../img/opacity-black.png');
	width: 100%;
	height: 100%;
	top: 0; left: 0;
	z-index: 99;
	img{
		position: absolute;
		margin: auto;
		top: 0; left: 0; bottom: 0; right: 0;
	}
}

#usersList{
	display: none;
}

///////////
// Login
///////////

#login{

	#loginForm{
		position: absolute;
		background: #ddd;
		width: 370px;
		height: 325px;
		margin: auto;
		top: 0; left: 0; right: 0; bottom: 0;
	}
	h1{
		background: #585c64;
		width: 100%;
		height: 60px;
		color: #ddd;
		font: bold 24px 'gotham';
		line-height: 60px;
		text-align: center;
	}

	#username, #password{
		width: 290px;
		height: 48px;
		padding-top: 5px;
		border: 1px solid #c2c2c2;
		color: #8d8d8d;
		text-indent: 50px;
		font: 14px 'gotham';
		line-height: 18px;
	}

	#username{
		background: #d3d3d3 url(../img/icon-user.png) 14px 12px no-repeat;
		margin: 40px 40px 0;
	}
	#password{
		background: #d3d3d3 url(../img/icon-key.png) 14px 12px no-repeat;
		margin: 18px 40px;
	}

	#submit{
		background: #585c64;
		width: 290px;
		height: 48px;
		border: none;
		cursor: pointer;
		color: white;
		font: bold 18px 'gotham';
		margin-left: 40px;
		@include transition(0.5s background);
		&:hover{ background: #727884; }
	}

	.fail{
		background: #8c5555 !important;
	}

	#shadow{
		background: url(../img/shadow.png);
		width: 515px;
		height: 129px;
		margin-top: 70px;
		margin-left: -63px;
	}
}

@-webkit-keyframes shake {
  0% { -webkit-transform: translate(2px, 1px) rotate(0deg); }
  10% { -webkit-transform: translate(-1px, -2px) rotate(-1deg); }
  20% { -webkit-transform: translate(-3px, 0px) rotate(1deg); }
  30% { -webkit-transform: translate(0px, 2px) rotate(0deg); }
  40% { -webkit-transform: translate(1px, -1px) rotate(1deg); }
  50% { -webkit-transform: translate(-1px, 2px) rotate(-1deg); }
  60% { -webkit-transform: translate(-3px, 1px) rotate(0deg); }
  70% { -webkit-transform: translate(2px, 1px) rotate(-1deg); }
  80% { -webkit-transform: translate(-1px, -1px) rotate(1deg); }
  90% { -webkit-transform: translate(2px, 2px) rotate(0deg); }
  100% { -webkit-transform: translate(1px, -2px) rotate(-1deg); } }

.shake {
  -webkit-animation-name: shake;
  -webkit-animation-duration: 0.8s;
  -webkit-transform-origin: 50% 50%;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-timing-function: linear; 
}








